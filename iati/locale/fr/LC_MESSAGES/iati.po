msgstr "[{\"id\": \"384d2d32-8994-43bf-b510-9e2c448e271e\", \"value\": \"<p>Vous pouvez maintenant accéder à l’ordre du jour (provisoire) et à tous les documents des réunions du Groupe des pays partenaires et du Comité directeur qui auront lieu du 14 au 16 octobre, à la Cité de l’ONU à Copenhague.</p>\", \"type\": \"paragraph\"}, {\"id\": \"f163adc6-97c6-4477-aadc-5fb7c4b1419c\", \"value\": [{\"id\": \"6715bd86-1d39-42cc-b4f1-c412a6289316\", \"value\": 393, \"type\": \"document\"}, {\"id\": \"58a5e190-18eb-4f72-a173-11493c243a49\", \"value\": 394, \"type\": \"document\"}], \"type\": \"document_box\"}, {\"id\": \"0fe19bae-94ba-416d-b43c-c415b9de226b\", \"value\": [{\"id\": \"fd37b72b-ca5d-430b-a59f-b8a8547e6a0f\", \"value\": \"Documents :\", \"type\": \"document_box_heading\"}, {\"id\": \"035d9ad6-9bdd-4218-9f25-ac1b2e5c2cb2\", \"value\": \"Document 1\", \"type\": \"document_box_heading\"}, {\"id\": \"3a40d5be-0675-450d-a7ca-12a27d07a30d\", \"value\": 363, \"type\": \"document\"}, {\"id\": \"6c880d15-3166-4dad-889a-b897c85eddb3\", \"value\": \"Document 2\", \"type\": \"document_box_heading\"}, {\"id\": \"1a209d94-0181-42d0-83a3-11b745e91a17\", \"value\": 364, \"type\": \"document\"}, {\"id\": \"1f847b8a-4b84-44af-b788-621433f9bbaf\", \"value\": 365, \"type\": \"document\"}, {\"id\": \"616fb4d0-3ca0-4e88-b84a-478428ffaeb9\", \"value\": \"Document 3\", \"type\": \"document_box_heading\"}, {\"id\": \"983d0f83-12f0-4ac2-9ada-dc38ca2469a7\", \"value\": 366, \"type\": \"document\"}, {\"id\": \"110f9114-8cb2-48b2-a6a8-e0ead5705c23\", \"value\": 367, \"type\": \"document\"}, {\"id\": \"ce6a8e2d-61d5-4cc4-b241-03c9b81e70fc\", \"value\": 368, \"type\": \"document\"}, {\"id\": \"44a11f91-c00d-4ade-bb03-21a25b514874\", \"value\": 369, \"type\": \"document\"}, {\"id\": \"0d365134-aa18-4784-b35d-ae31bde0696e\", \"value\": 370, \"type\": \"document\"}, {\"id\": \"30ca1269-d798-4bf5-8448-440e6c4580cb\", \"value\": \"Document 4\", \"type\": \"document_box_heading\"}, {\"id\": \"fd6c1
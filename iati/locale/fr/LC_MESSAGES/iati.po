msgid "[{\"id\": \"c38c7dae-ebab-4158-a583-f4d2150a2055\", \"value\": \"<p></p><h4><b>Governing Board meeting minutes</b></h4><p>IATI\\u2019s governing board aims to meet in person at least once a year, and through online conference calls bimonthly. Minutes from those meetings, and updates issued by the board, are available to download.</p>\", \"type\": \"intro\"}, {\"id\": \"dd4b689f-e096-4c1e-b01f-0d51ce68a8a3\", \"value\": [{\"id\": \"74deb598-187b-4c41-b321-af4d6e0ffebd\", \"value\": \"2018\", \"type\": \"document_box_heading\"}, {\"id\": \"6e87d5ae-8b4a-4f42-9c5a-be4228dc44a1\", \"value\": 496, \"type\": \"document\"}, {\"id\": \"7b5fce43-e436-4040-9886-8e11e4744b1e\", \"value\": 495, \"type\": \"document\"}, {\"id\": \"091bc7d2-d375-4935-bae3-1d33f3da6fb1\", \"value\": 497, \"type\": \"document\"}, {\"id\": \"f25c4ac6-5fb4-4fc1-bc84-89db39714692\", \"value\": 33, \"type\": \"document\"}, {\"id\": \"4e8f28b4-26d0-45aa-8ac3-59c0bf2d9e8c\", \"value\": \"2017\", \"type\": \"document_box_heading\"}, {\"id\": \"1647c61b-3bbb-4237-b308-b706532695f0\", \"value\": 37, \"type\": \"document\"}, {\"id\": \"fe6580ad-2176-4ab8-91f5-54b7467860cf\", \"value\": 36, \"type\": \"document\"}, {\"id\": \"7d6304d0-72d3-4fcc-93a2-fa6a1ea4e58e\", \"value\": 35, \"type\": \"document\"}, {\"id\": \"9159f5dc-4192-4eb1-96aa-cf34c96a1f54\", \"value\": 47, \"type\": \"document\"}, {\"id\": \"967a8588-bf19-47ed-ba28-35c5191c8ec6\", \"value\": 41, \"type\": \"document\"}, {\"id\": \"a29d5b04-e577-4d07-a53b-5bd0736d5702\", \"value\": 48, \"type\": \"document\"}, {\"id\": \"fac51d76-18e9-4768-aa2b-b0c2a1281d32\", \"value\": 40, \"type\": \"document\"}, {\"id\": \"e94c163e-fc8f-4646-b516-2bfd88cfd6ca\", \"value\": 46, \"type\": \"document\"}, {\"id\": \"e4f56e23-4ba6-4f45-a31d-32c11cda93b9\", \"value\": \"2016\", \"type\": \"document_box_heading\"}, {\"id\": \"883588f3-d683-4eef-8d00-3b0b5cd3e8eb\", \"value\": 39, \"type\": \"document\"}, {\"id\": \"6be1b504-271d-4f84-8137-cb5f5827e24a\", \"val
#. events.eventpage.458.content_editor
msgid "[{\"id\": \"c039a703-8114-49fb-95ff-8146124337af\", \"value\": \"<p>The IATI Secretariat are pleased to share the papers and <a href=\\\"https://www.aidtransparency.net/wp-content/uploads/2010/05/Agenda-for-the-7th-Steering-Committee-of-the-International-Aid-Transparency-Initiative_P1.doc\\\">Agenda</a> for the Steering Committee Meeting on Monday 25th October, 9:00 am, OECD, Paris. The agenda includes:</p><h3>1. Framework for Implementation</h3><h3>2. Post HLF4</h3><h3>3. Technical items for agreement</h3><ul><li>Licensing</li><li>Geo-coding</li><li>Organisational IDs</li><li>Multi-funded Activities</li></ul><p>Preparation Papers</p><ul><li><a href=\\\"https://www.aidtransparency.net/wp-content/uploads/2010/05/IATI-POST-2011-QUESTIONS-FOR-IATI-STEERING-COMMITTEE_P11.doc\\\">IATI Post 2011</a></li><li><a href=\\\"https://www.aidtransparency.net/wp-content/uploads/2010/05/IATI-SC-Meeting-25-October-2010-Revised-draft-Framework-for-Implementation_P1.doc\\\">Revised draft of Framework for Implementation</a></li><li>Summary of October 2010 TAG Meeting</li><li>2011 TAG Workplan</li><li>Adjustments to Phase 1 Standards and Phase 2 Additions</li><li>Donor Implementation Schedule</li><li>Licensing Proposal</li></ul><p>For more information please contact <a href=\\\"mailto:Neil-McKie@dfid.gov.uk\\\">Neil McKie</a>.</p>\", \"type\": \"paragraph\"}, {\"id\": \"4faff69a-cb64-42ee-ae4f-d1191a4c9019\", \"value\": [{\"id\": \"df7b6c97-39f2-4e62-9131-7372875d4f84\", \"value\": \"Documents:\", \"type\": \"document_box_heading\"}, {\"id\": \"d56dedd7-c028-4383-a89c-ff19d948b4a8\", \"value\": 466, \"type\": \"document\"}, {\"id\": \"5ddbae3d-0e3c-45bf-85e5-5c7885129f79\", \"value\": 467, \"type\": \"document\"}, {\"id\": \"45e1074c-02ad-4a6c-9338-4f176fc34e4f\", \"value\": 468, \"type\": \"document\"}, {\"id\": \"78a60c84-3156-4f3d-a4ae-a5800563b8c0\", \"value\": 469, \"type\": \"document\"}, {\"id\": \"cc816fe6-a524-4c58-8392-88d16f307c1d\", \"value\": 198, \"type\": \"document\"}, {\"id\": \"c9c4bdc6-54b3-4ab8-ad2f-9dfd7b4b1ff3\", \"value\": 570, \"type\": \"document\"}, {\"id\": \"02900923-8cfb-4a1a-afe3-98944c0824fa\", \"value\": 571, \"type\": \"document\"}, {\"id\": \"8c08a0cc-d340-4502-9107-ed99347b2f7c\", \"value\": 572, \"type\": \"document\"}, {\"id\": \"d2eaeb08-8ccb-4e9b-a369-12743d5eaa19\", \"value\": 574, \"type\": \"document\"}, {\"id\": \"ba793961-522c-4ef1-811f-1a956d5a4a33\", \"value\": 576, \"type\": \"document\"}], \"type\": \"document_box\"}]"

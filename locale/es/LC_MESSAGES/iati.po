#. about.aboutsubpage.534.content_editor
msgid "[{\"id\": \"83fa545b-de06-41f6-aa9f-4d5556865d8f\", \"value\": \"Steering Committee sub-groups archive\", \"type\": \"h2\"}, {\"id\": \"2bcd8fff-1cbf-44e3-a0fc-54c95fcf06c0\", \"value\": \"<p>IATI\\u2019s Steering Committee has been replaced by a new <a id=\\\"40\\\" linktype=\\\"page\\\">Members\\u2019 Assembly</a> established in April 2016 as part of new governance arrangements.</p><p>An archive of documents and meetings notes from the Steering Committee\\u2019s sub-groups can be found below.</p>\", \"type\": \"paragraph\"}, {\"id\": \"7126a857-f088-4a55-a81b-9215d56adecc\", \"value\": \"Members' Advisory Group (MAG)\", \"type\": \"h3\"}, {\"id\": \"e7db6539-3a7c-4998-bfee-498e964a1218\", \"value\": \"<ul><li><b>Members</b>: African Development Bank, Bangladesh, Bill and Melinda Gates Foundation, Bond, DFID</li><li><b>Chair</b>: Robin Uyterlinde, Netherlands (up until December 2015)</li></ul>\", \"type\": \"paragraph\"}, {\"id\": \"a136cbfa-35a2-4611-bdb4-699254b04142\", \"value\": [{\"id\": \"fb2178af-c08d-4410-b306-ad4b43f45a92\", \"value\": 632, \"type\": \"document\"}, {\"id\": \"59ee9511-432d-444d-894a-6bd3d8e4b969\", \"value\": 622, \"type\": \"document\"}, {\"id\": \"68d8bd48-8cee-4e5f-95cc-a78fb7d4b92f\", \"value\": 628, \"type\": \"document\"}, {\"id\": \"244d3aca-a8f6-407e-918b-579090e00d24\", \"value\": 621, \"type\": \"document\"}, {\"id\": \"78e6815a-3406-4802-b0cf-7e2306ed5169\", \"value\": 631, \"type\": \"document\"}, {\"id\": \"14984565-6f37-4b70-be7a-19b65fb3c0c4\", \"value\": 627, \"type\": \"document\"}, {\"id\": \"aba31f08-21c1-49a4-a173-d5715dee1faf\", \"value\": 626, \"type\": \"document\"}, {\"id\": \"ca4fb029-9ed2-4e7c-881c-f738e18f2fbd\", \"value\": 624, \"type\": \"document\"}, {\"id\": \"10df757e-933a-476a-b37f-63188f50af12\", \"value\": 629, \"type\": \"document\"}, {\"id\": \"830b1c7c-b2ef-41b5-ac8b-6d3babf05a87\", \"value\": 625, \"type\": \"document\"}, {\"id\": \"87d73c44-82e3-4adf-819c-9664bc3116f2\", \"value\": 623, \"type\": \"document\"}, {\"id\": \"aa70c33a-1156-4fdb-960f-12cee9c948ed\", \"value\": 630, \"type\": \"document\"}], \"type\": \"document_box\"}, {\"id\": \"02abe6e6-3e14-4219-acb8-35f3f74c77cf\", \"value\": \"Budget and Finance sub-group\", \"type\": \"h3\"}, {\"id\": \"a1a56b06-e432-4bfb-a265-225493b7089a\", \"value\": \"<ul><li><b>Members</b>: Canada, Madagascar, Transparency International, World Bank</li><li><b>Chair</b>: Martin Akerman, UNFPA</li></ul>\", \"type\": \"paragraph\"}, {\"id\": \"0362280e-ce9b-42e6-91c3-dc31f19255b2\", \"value\": [{\"id\": \"1596819d-a46c-47f6-aa59-306cde97d36a\", \"value\": 636, \"type\": \"document\"}, {\"id\": \"b499603a-3bb6-4538-a2c2-59f7547f8b21\", \"value\": 637, \"type\": \"document\"}, {\"id\": \"3bace9a7-d7d7-4764-b57f-14ae398ca205\", \"value\": 635, \"type\": \"document\"}, {\"id\": \"4eb8d0cb-600f-4cf7-b7c0-fd2859ea092f\", \"value\": 634, \"type\": \"document\"}, {\"id\": \"09776d70-290f-4910-b383-b3c1168238d7\", \"value\": 633, \"type\": \"document\"}], \"type\": \"document_box\"}]"
